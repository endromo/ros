{{page-title "Show"}}

<br>
<br>

{{#if this.model}}
<div class="col-12 col-md-8 col-lg-4 mx-auto my-4">

    <h2>Customer Fetch</h2>
    <table align="center">
        {{#each this.model as |item|}}            
        <tr>
            <td>Username</td>
            <td>:</td>
            <td>{{item.username}}</td>
        </tr>
        <tr>
            <td>Password</td>
            <td>:</td>
            <td>{{item.pwd}}</td>
        </tr>
        <tr>
            <td>
                <hr>
            </td>
        </tr>
        {{/each}}     
    </table>
</div>
{{else}}

<h2>.Empty.</h2>

{{/if}}